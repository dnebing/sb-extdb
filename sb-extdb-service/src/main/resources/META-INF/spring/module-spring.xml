<?xml version="1.0"?>

<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<bean class="com.liferay.example.servicebuilder.extdb.service.impl.UserLoginLocalServiceImpl" id="com.liferay.example.servicebuilder.extdb.service.UserLoginLocalService" />
	<bean class="com.liferay.example.servicebuilder.extdb.service.persistence.impl.UserLoginPersistenceImpl" id="com.liferay.example.servicebuilder.extdb.service.persistence.UserLoginPersistence">
		<property name="dataSource" ref="extDataSource" />
	</bean>
</beans>